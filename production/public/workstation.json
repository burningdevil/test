{
  "version": "1.0.0",
  "name": "HomeScreen Admin",
  "isEnabled": true,
  "contributions": {
    "smartFolders": {
      "items": [
        {
          "key": "applicationSmartFolderKey",
          "command": "openConfigMain",
          "ribbonColor": "#1C8DD4",
          "location": {
            "section": "Administration",
            "index": 5
          },
          "icon": "Applications.png",
          "selectedIcon": "Applications.png",
          "showEnvironmentSelector": true
        }
      ]
    },
    "menus": {
      "items": [
        {
          "key": "newApplication",
          "command": "openNewConfig",
          "icon": "Applications.png",
          "windowKey": "homescreenConfigEditor",
          "isVisible": "isHomeScreenConfigEnabled",
          "canExecute": "isHomeScreenConfigEnabled"
        },
        {
          "key": "editApplication",
          "command": "openEditConfig",
          "icon": "Applications.png",
          "windowKey": "homescreenConfigEditor",
          "isVisible": "isHomeScreenConfigEnabled",
          "canExecute": "isHomeScreenConfigEnabled"
        },
        {
          "key": "openAppDesign",
          "command": "openAppDesignEditor",
          "windowKey": "appDesignEditor",
          "isVisible": "isHomeScreenConfigEnabled",
          "canExecute": "isHomeScreenConfigEnabled"
        }
      ]
    },
    "objectEditors": [
      {
        "objectType": "HomeScreenConfig",
        "item": "editApplication",
        "editorTypes": ["Edit"]
      },
      {
        "objectType": "HomeScreenConfig",
        "item": "newApplication",
        "editorTypes": ["Create"]
      },
      {
        "objectType": "ApplicationDesign",
        "item": "openAppDesign",
        "editorTypes": ["Create", "Edit"]
      }
    ],
    "windows": [
      {
        "key": "homescreenConfigEditor",
        "showMenus": false,
        "size": {
          "allowResize": false,
          "minimumHeight": 708,
          "minimumWidth": 962,
          "initialHeight": 708,
          "initialWidth": 962
        }
      },
      {
        "key": "appDesignEditor",
        "isModal": true,
        "showMenus": false,
        "size": {
          "allowResize": false,
          "minimumHeight": 708,
          "minimumWidth": 962,
          "initialHeight": 708,
          "initialWidth": 962
        }
      }
    ]
  }
}
