  "description": "",
  "author": "",
  "license": "MIT",
  "keywords": [
    "react-component"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "umd",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "start": "webpack-dev-server --config example/webpack.config.js",
    "build": "yarn build-lib && yarn build-es && yarn build-umd",
    "build-lib": "rimraf lib && babel src --out-dir lib --copy-files --ignore test.js",
    "build-es": "rimraf es && BABEL_ENV=es babel src --out-dir es --copy-files --ignore test.js",
    "build-umd": "rimraf umd && webpack",
    "build-example": "rimraf example/dist && webpack --mode production --config example/webpack.config.js",
    "clean": "rimraf lib es umd example/dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
